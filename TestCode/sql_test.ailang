LibraryImport.SmolLM_SQL

PrintMessage("Testing SQL library standalone...\n")

// Connect
RunTask(SmolLM_ConnectDB)

// Set token_id
token_id = 42

PrintMessage("Loading embedding for token 42...\n")

// Load embedding
RunTask(SmolLM_LoadEmbedding)

PrintMessage("SUCCESS! Loaded embedding.\n")
PrintMessage("d0 = ")
PrintNumber(CurrentEmbedding.d0)
PrintMessage("\n")

// Disconnect
RunTask(SmolLM_DisconnectDB)

PrintMessage("Test complete!\n")