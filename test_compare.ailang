// test_compare.ailang
// Tests all comparison operations for self-hosting compiler
// Expected output: Series of 1s and 0s showing comparison results

SubRoutine.Main {
    
        PrintMessage("=== Comparison Tests ===\n")
        
        // EqualTo tests
        PrintMessage("EqualTo(5, 5): ")
        result = EqualTo(5, 5)
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("EqualTo(5, 3): ")
        result = EqualTo(5, 3)
        PrintNumber(result)
        PrintMessage(" (expect 0)\n")
        
        // NotEqual tests
        PrintMessage("NotEqual(5, 3): ")
        result = NotEqual(5, 3)
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("NotEqual(5, 5): ")
        result = NotEqual(5, 5)
        PrintNumber(result)
        PrintMessage(" (expect 0)\n")
        
        // LessThan tests
        PrintMessage("LessThan(3, 5): ")
        result = LessThan(3, 5)
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("LessThan(5, 3): ")
        result = LessThan(5, 3)
        PrintNumber(result)
        PrintMessage(" (expect 0)\n")
        
        PrintMessage("LessThan(5, 5): ")
        result = LessThan(5, 5)
        PrintNumber(result)
        PrintMessage(" (expect 0)\n")
        
        // GreaterThan tests
        PrintMessage("GreaterThan(5, 3): ")
        result = GreaterThan(5, 3)
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("GreaterThan(3, 5): ")
        result = GreaterThan(3, 5)
        PrintNumber(result)
        PrintMessage(" (expect 0)\n")
        
        PrintMessage("GreaterThan(5, 5): ")
        result = GreaterThan(5, 5)
        PrintNumber(result)
        PrintMessage(" (expect 0)\n")
        
        // LessEqual tests
        PrintMessage("LessEqual(3, 5): ")
        result = LessEqual(3, 5)
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("LessEqual(5, 5): ")
        result = LessEqual(5, 5)
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("LessEqual(5, 3): ")
        result = LessEqual(5, 3)
        PrintNumber(result)
        PrintMessage(" (expect 0)\n")
        
        // GreaterEqual tests
        PrintMessage("GreaterEqual(5, 3): ")
        result = GreaterEqual(5, 3)
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("GreaterEqual(5, 5): ")
        result = GreaterEqual(5, 5)
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("GreaterEqual(3, 5): ")
        result = GreaterEqual(3, 5)
        PrintNumber(result)
        PrintMessage(" (expect 0)\n")
        
        // Nested comparisons with arithmetic
        PrintMessage("\n=== Nested Tests ===\n")
        
        PrintMessage("EqualTo(Add(2, 3), 5): ")
        result = EqualTo(Add(2, 3), 5)
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("LessThan(Add(1, 1), Multiply(2, 3)): ")
        result = LessThan(Add(1, 1), Multiply(2, 3))
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("GreaterThan(Multiply(3, 4), Add(5, 5)): ")
        result = GreaterThan(Multiply(3, 4), Add(5, 5))
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        // Negative numbers (signed comparison)
        PrintMessage("\n=== Signed Tests ===\n")
        
        PrintMessage("LessThan(-5, 0): ")
        result = LessThan(Negate(5), 0)
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("GreaterThan(0, -5): ")
        result = GreaterThan(0, Negate(5))
        PrintNumber(result)
        PrintMessage(" (expect 1)\n")
        
        PrintMessage("\n=== All Tests Complete ===\n")
    
}
RunTask(Main)