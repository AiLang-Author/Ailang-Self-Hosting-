SubRoutine.TestVariables {
    x = 10
    y = 20
    result = Add(x, y)
    PrintNumber(result)
}

SubRoutine.TestIf {
    value = 5
    IfCondition GreaterThan(value, 3) ThenBlock: {
        PrintMessage("Value is greater than 3\n")
    }
}

SubRoutine.Main {
    PrintMessage("Starting tests\n")
    RunTask(TestVariables)
    RunTask(TestIf)
    PrintMessage("Tests complete\n")
}

RunTask(Main)