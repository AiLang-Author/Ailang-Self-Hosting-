// Test CParserTypes - just the type definitions and pools
LibraryImport.Compiler.Frontend.Parser.CParserTypes

SubRoutine.Main {
    PrintMessage("=== CParserTypes Test ===\n")
    
    // Test ASTNode constants
    PrintMessage("ASTNode.PROGRAM = ")
    PrintNumber(ASTNode.PROGRAM)
    PrintMessage("\n")
    
    PrintMessage("ASTNode.FUNCTION = ")
    PrintNumber(ASTNode.FUNCTION)
    PrintMessage("\n")
    
    PrintMessage("ASTNode.IDENTIFIER = ")
    PrintNumber(ASTNode.IDENTIFIER)
    PrintMessage("\n")
    
    // Test Parser pool
    PrintMessage("Parser.pos = ")
    PrintNumber(Parser.pos)
    PrintMessage("\n")
    
    PrintMessage("Parser.node_count = ")
    PrintNumber(Parser.node_count)
    PrintMessage("\n")
    
    // Test NodeConfig
    PrintMessage("NodeConfig.MAX_CHILDREN = ")
    PrintNumber(NodeConfig.MAX_CHILDREN)
    PrintMessage("\n")
    
    // Test Precedence
    PrintMessage("Precedence.PRIMARY = ")
    PrintNumber(Precedence.PRIMARY)
    PrintMessage("\n")
    
    PrintMessage("=== CParserTypes PASS ===\n")
}

RunTask(Main)